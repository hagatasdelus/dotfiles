### alias ###
alias nl='nl -ba -s ": "'
alias tree='sh ~/bin/tree.sh'
alias nv='~/nvim-macos-x86_64/bin/nvim'
alias nvim='~/nvim-macos-x86_64/bin/nvim'
alias closesns='sh ~/bin/close_sns_apps_in_macos.sh'
alias ntf='sh ~/bin/notification.sh'
alias edit='open -a "Visual Studio Code" '
alias la='ls -A'
alias h='history'
alias h2='history 20'
alias la='ls -laO'
alias ll='ls -l'
# alias auth='sh ~/bin/auth.sh'
alias java_home-v='sh ~/bin/java_home-v.sh'
alias path='echo $PATH | tr ":" "\n"'

alias git-commit-setup='~/.config/git/scripts/setup-commit-template.sh'


if [ -f ~/.bashrc.local ]; then
    source ~/.bashrc.local
fi

